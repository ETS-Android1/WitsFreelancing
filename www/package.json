{
  "name": "wits_freelance",
  "displayName": "Wits Freelance",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "../www/test/test.js",
  "scripts": {
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "start": "../www/test/test.js",
    "test": "mocha --recursive --reporter mocha-junit-reporter --timeout 60000 --exit"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "chai": "^4.2.0",
    "cordova-android": "^8.0.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-advanced-http": "^2.1.1",
    "cordova-plugin-file": "^6.0.2",
    "express": "^4.17.1",
    "mocha": "^6.2.0"
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-advanced-http": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skhu47/WitsFreelancing.git"
  },
  "bugs": {
    "url": "https://github.com/Skhu47/WitsFreelancing/issues"
  },
  "homepage": "https://github.com/Skhu47/WitsFreelancing#readme",
